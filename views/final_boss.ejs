<!DOCTYPE html>
<html lang="en">

<head>
    <title>Password Pet</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/final_boss.css">
    <link rel="stylesheet" href="/stylesheets/navbar.css">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>

<body>
    <!-----------navbar---------------------------->
    <%- include('./partials/navbar.ejs') %>
   
   

    <section>
        <div class="main" id="dragon_box">
            <div class="mt-4 text-center">
            <h2 class="custom_placeholder"><%=user.username%>'s Dragon:</h2>
                <img id= "strength_img" src="./images/EGG.png" alt="Dragon" height="450">
                </div>
        </div>
    </section>
    <section>
        <div class="mt-4 text-center">
            <button class="custom_button" id="checkPasswordBtn">Check Password</button>
            <p id="result"></p>
    </div>
    </section>


    <section>
        <div class="main" id="final_boss_box">
            <div class="mt-4 text-center">
            <h2 class="custom_placeholder">Brute Force Attack!</h2>
                <img id= "strength_img center-text" src="./images/Hacker.png" alt="Hacker" height="450">
                </div>
        </div>
    </section>

    <script src="/javascripts/final_boss.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
   
   <!-- <footer class="mt-auto text-center py-3">
        <a href="https://www.vecteezy.com/free-videos/data">Data Stock Videos by Vecteezy</a>
